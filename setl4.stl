	setl4.kind(kind,'map')				:s(setl4.reduce.map)
	setl4.kind(kind,'seq')				:s(setl4.reduce.seq)
	setl4.kind(kind,'set')				:s(setl4.reduce.set)
	setl4.kind(kind,'str')				:s(setl4.reduce.str)

setl4.reduce.map
	setl4.reduce = set.new(#set)
	loop(set)

setl4.reduce.map.next

	member = next(#set)			        :f(return)
	member = eval(expr)				:f(setl4.reduce.map.next)
	eq(member)					:s(setl4.reduce.map.next)
	set.add(setl4.reduce,member)			:(setl4.reduce.map.next)


setl4.reduce.seq

	setl4.reduce = seq.new(#set)
	loop(set)


setl4.reduce.seq.next

	member = next(#set)			        :f(return)
	member = eval(expr)				:f(seql4.reduce.seq.next)
	eq(member)					:s(seql4.reduce.seq.next)
	seq.add(setl4.reduce,member)			:(setl4.reduce.seq.next)

