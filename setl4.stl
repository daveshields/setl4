	member = next(set)			        :f(return)

	ouput = '4.reduce|' member '|'
	r = eval(expr)				        :f(setl4.reduce.set.next)
	eq(r)					        :s(setl4.reduce.set.next)
	set.add(setl4.reduce,member)		        :(setl4.reduce.set.next)

