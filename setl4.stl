	define('visit(obj,expr)')			:(visit.end)


*	_visit_ loops over _obj_ and calls _eval_ with argument 
*	_expr_  for each member so reached.

visit

	loop(obj)

visit.next

	next(obj)					:f(return)
	output = 'next ' set.next(obj)
	eval(expr)					:(visit.next)
	
visit.end

