rank.map.next

	entry = ara[i = i + 1,1]			:f(return)
	map.put(rank,key(entry),value(entry))		:(rank.map.next)
	
rank.set.next

	key = ara[i = i + 1,1]				:f(return)
	set.add(rank,key)				:(rank.set.next)
	
